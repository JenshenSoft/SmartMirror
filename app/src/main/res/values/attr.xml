<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClockView">
        <attr name="showSecondHand" format="boolean" />
        <attr name="showDots" format="boolean" />
        <attr name="showNumerals" format="boolean" />
        <attr name="showDigitalClock" format="boolean" />
        <attr name="second" format="integer" />
        <attr name="minute" format="integer" />
        <attr name="hour" format="integer" />
        <attr name="secondHandColor" format="color" />
        <attr name="minuteHandColor" format="color" />
        <attr name="dotColor" format="color" />
        <attr name="numeralColor" format="color" />
        <attr name="hourHandColor" format="color" />
        <attr name="digitalClockTextColor" format="color" />
        <attr name="circleColor" format="color" />
    </declare-styleable>
</resources>